{
  "expo": {
    "name": "car-finder",
    "slug": "car-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(Car Finder) to use your location."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.carfinder",
      "config":{
        "googleMapsApiKey" :"AIzaSyDBChCOptsRLGhjUoFKqt9aNWJcGv2NLpU"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carfinder",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDBChCOptsRLGhjUoFKqt9aNWJcGv2NLpU"
        },
        "permissions": ["ACCESS_COARSE_LOCATION","ACCESS_FINE_LOCATION","FOREGROUND_SERVICE"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
